 @model IEnumerable<mohua_web_.Entities.TblNulmApiSusvcov>



<table id="example1" class="table align-items-center mb-0" style="overflow-x: auto; display: block; white-space: nowrap;">
    <thead>
        <tr>
            <th>State Name</th>
            <th>District Name</th>
            <th>ULB Name</th>
            <th>City Survey</th>
            <th>No. of ID Cards Issued</th>
            <th>No. of Vendors Markets Completed</th>
            <th>No. of Vending Zones</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var member in Model)
        {
            <tr>
                <td>@member.StateName</td>
                <td>@member.DistrictName</td>
                <td>@member.UlbName</td>
                <td>@member.CitySurvey</td>
                <td>@member.NoOfIdcardIssued</td>
                <td>@member.NoofVendorsMarketsCompleted</td>
                <td>@member.Novendingzone</td>
            </tr>
        }
    </tbody>
</table>







<!-- Page specific script -->
<script>
    $(function () {
        $("#example1").DataTable({
            "responsive": true,
            "lengthChange": false,
            "autoWidth": false,
            "paging": true,
            "ordering": true,
            "info": true,
            "buttons": ["copy", "csv", "excel", "pdf", "print", "colvis"]
        }).buttons().container().appendTo('#example1_wrapper .col-md-6:eq(0)');
    });

</script>
