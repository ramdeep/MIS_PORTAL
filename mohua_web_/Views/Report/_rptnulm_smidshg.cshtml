 @model IEnumerable<mohua_web_.Entities.TblNulmApiSmidshg>



<table id="example1" class="table align-items-center mb-0" style="overflow-x: auto; display: block; white-space: nowrap;">
    <thead>
        <tr>
            <th>State Name</th>
            <th>District Name</th>
            <th>ULB Name</th>
            <th>SHG Group Code</th>
            <th>SHG Name</th>
            <th>SHG DOF</th>
            <th>SHG Type</th>
            <th>RF Status</th>
            <th>Disbursement Date</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var member in Model)
        {
            <tr>
                <td>@member.StateName</td>
                <td>@member.DistrictName</td>
                <td>@member.UlbName</td>
                <td>@member.ShgGroupcode</td>
                <td>@member.ShgName</td>
                <td>@member.ShgDof</td>
                <td>@member.ShgType</td>
                <td>@member.RfStatus</td>
                <td>@member.DisbursedmentDate</td>
            </tr>
        }
    </tbody>
</table>






<!-- Page specific script -->
<script>
    $(function () {
        $("#example1").DataTable({
            "responsive": true,
            "lengthChange": false,
            "autoWidth": false,
            "paging": true,
            "ordering": true,
            "info": true,
            "buttons": ["copy", "csv", "excel", "pdf", "print", "colvis"]
        }).buttons().container().appendTo('#example1_wrapper .col-md-6:eq(0)');
    });

</script>
