@model IEnumerable<mohua_web_.Entities.TblAmrutsummary>



@* <table class="table" id="example1" table-responsive"> paging aur export*@
<table id="example1" class="table table-bordered table-striped Items table-responsive" style="overflow-x: auto; display: block; white-space: nowrap;">
    <thead>
        <tr>
            <th>S.No</th>
            <th>State Name</th>
            <th>Central Assistance Outlay Increment</th>
            <th>Central Assistance Approved Increment</th>
            <th>Approved Projects</th>
            <th>Approved Project Cost Increment</th>
            <th>DPR Approved Project Count</th>
            <th>DPR Approved Project Cost Increment</th>
            <th>NIT Issued Project Count</th>
            <th>NIT Issued Project Cost Increment</th>
            <th>Contract Awarded Project Count</th>
            <th>Contract Awarded Project Cost Increment</th>
            <th>Project Count</th>
            <th>Project Cost Increment</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var member in Model)
        {
            <tr>
                <td>@member.Sno</td>
                <td>@member.Statename</td>
                <td>@member.Centralassistanceoutlayincr</td>
                <td>@member.Centralassistanceapprovedincr</td>
                <td>@member.Approvedprojects</td>
                <td>@member.Approvedprojectcostincr</td>
                <td>@member.Dprapprovedprojectcount</td>
                <td>@member.Dprapprovedprojectcostincr</td>
                <td>@member.Nitissuedprojectcount</td>
                <td>@member.Nitissuedprojectcostincr</td>
                <td>@member.Contractawardedprojectcount</td>
                <td>@member.Contractawardedprojectcostincr</td>
                <td>@member.Projectcount</td>
                <td>@member.Projectcostincr</td>
            </tr>
        }
    </tbody>
</table>




<!-- Page specific script -->

    <script>
        $(document).ready(function () {
            var table = $('#example1').DataTable({
            //Toggle the below paging to false - to remove pagination
            paging: true,

        //Nishant Code Start - Code Add for Exporting to PDF
        dom: 'Bfrtip',
        buttons: [
        {
            extend: 'excelHtml5',
        title: 'AMRUT SUMMERY REPORT',
        text: 'Export to Excel'
                    }
        ]
                //Code End - Code Added for File Export to PDF

            });

        $('a.toggle-vis').on('click', function (e) {
            e.preventDefault();

        // Get the column API object
        var column = table.column($(this).attr('data-column'));

        // Toggle the visibility
        column.visible(!column.visible());
            });
        });
</script>


